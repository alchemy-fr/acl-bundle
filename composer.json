{
  "name": "alchemy/acl-bundle",
  "type": "symfony-bundle",
  "description": "Symfony ACL bundle",
  "keywords": [],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "doctrine/orm": "^2.6",
    "ramsey/uuid-doctrine": "^1.5",
    "symfony/event-dispatcher": "^4.0||^5",
    "symfony/framework-bundle": "^4.0||^5",
    "symfony/security-bundle": "^4.0||^5",
    "symfony/yaml": "^6.2"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\AclBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\AclBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "phpunit/phpunit": "^8.4"
  }
}
